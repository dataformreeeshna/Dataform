config {
  type: "table",
  description: "This table calculates AVG from the source view",
  name: "Dataform_results"
}
select  
t1.CustomerID,t2.OrderID,Sum(t3.Quantity * t3.Unitprice) as Total
FROM ${ref("customer")} t1
join ${ref("orders")} t2  
on t1.CustomerID = t2.CustomerID
join ${ref("order_details")} t3  
on t2.OrderID = t3.OrderID
group By t1.CustomerID,t2.OrderID
having Total >= 10000